#: ../panel-plugin/pulseaudio-menu.c:254
msgid "Error"
msgstr "Fout"

#: ../panel-plugin/pulseaudio-button.c:336
#: ../panel-plugin/pulseaudio-notify.c:199
#, c-format
msgid "Not connected to the PulseAudio server"
msgstr "Niet verbonden met de PulseAudio-server"

#: ../panel-plugin/pulseaudio-button.c:354
#, c-format
msgid ""
"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Volume %d%% (gedempt)</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-button.c:356
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Volume %d%%</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-menu.c:521
msgid "Output"
msgstr "Uitvoer"

#: ../panel-plugin/pulseaudio-menu.c:561
msgid "Input"
msgstr "Invoer"

#: ../panel-plugin/pulseaudio-menu.c:663
msgid "Choose Playlist"
msgstr "Kies speellijst"

#. Audio mixers
#: ../panel-plugin/pulseaudio-menu.c:698
msgid "_Audio mixer..."
msgstr "_Geluidmengpaneel...."

