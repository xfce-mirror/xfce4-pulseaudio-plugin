#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Âm lượng %d%%</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-menu.c:483
msgid "Port"
msgstr ""

#: ../panel-plugin/pulseaudio-menu.c:584
msgid "Output"
msgstr "Đầu ra"

#: ../panel-plugin/pulseaudio-menu.c:628
msgid "Input"
msgstr "Đầu vào"

#: ../panel-plugin/pulseaudio-menu.c:730
msgid "Choose Playlist"
msgstr "Chọn danh sách phát"

