"<small>%s</small>"
msgstr "<b>Glasnost %d%% (utišano)</b>\n<small>%s</small>"

#: panel-plugin/pulseaudio-button.c:368
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Glasnost %d%%</b>\n<small>%s</small>"

