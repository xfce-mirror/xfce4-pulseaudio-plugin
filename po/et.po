"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Helivaljus %d%% (summutatud)</b>\n<small>%s</small>"

#: panel-plugin/pulseaudio-button.c:368
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Helivaljus %d%%</b>\n<small>%s</small>"

