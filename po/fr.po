#: ../panel-plugin/pulseaudio-menu.c:251
#, c-format
msgid ""
"<big><b>Failed to execute command \"%s\".</b></big>\n"
"\n"
"%s"
msgstr "<big><b>L’exécution de la commande « %s » a échoué.</b></big>\n\n%s"

#: ../panel-plugin/pulseaudio-dialog.c:153
#: ../panel-plugin/pulseaudio-menu.c:254
msgid "Error"
msgstr "Erreur"

#: ../panel-plugin/pulseaudio-button.c:336
#: ../panel-plugin/pulseaudio-notify.c:199
#, c-format
msgid "Not connected to the PulseAudio server"
msgstr "Non connecté au serveur PulseAudio"

#: ../panel-plugin/pulseaudio-button.c:354
#, c-format
msgid ""
"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Volume %d%% (muet)</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-button.c:356
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Volume %d%%</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-menu.c:521
msgid "Output"
msgstr "Sortie"

#: ../panel-plugin/pulseaudio-menu.c:561
msgid "Input"
msgstr "Entrée"

#: ../panel-plugin/pulseaudio-menu.c:663
msgid "Choose Playlist"
msgstr "Choisir la liste de lecture"

#. Audio mixers
#: ../panel-plugin/pulseaudio-menu.c:698
msgid "_Audio mixer..."
msgstr "Mixeur _audio…"

#: ../panel-plugin/pulseaudio-notify.c:201
#, c-format
