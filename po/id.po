#, c-format
msgid ""
"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Volume%d%%(senyapkan)</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-button.c:372
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Volume%d%%</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-menu.c:483
msgid "Port"
msgstr ""

#: ../panel-plugin/pulseaudio-menu.c:584
msgid "Output"
msgstr "keluaran"

#: ../panel-plugin/pulseaudio-menu.c:628
msgid "Input"
msgstr "Masukan"

#: ../panel-plugin/pulseaudio-menu.c:730
msgid "Choose Playlist"
msgstr "Pilih Senarai Putar"

