#: ../panel-plugin/pulseaudio-dialog.c:143
#: ../panel-plugin/pulseaudio-menu.c:278
msgid "Error"
msgstr "Error"

#: ../panel-plugin/pulseaudio-button.c:352
#: ../panel-plugin/pulseaudio-notify.c:188
#, c-format
msgid "Not connected to the PulseAudio server"
msgstr "Sin conexión con el servidor PulseAudio"

#: ../panel-plugin/pulseaudio-button.c:370
#, c-format
msgid ""
"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Volume %d%% (silenciado) </b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-button.c:372
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Volumen %d %%</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-menu.c:483
msgid "Port"
msgstr "Puerto"

#: ../panel-plugin/pulseaudio-menu.c:584
msgid "Output"
msgstr "Salida"

#: ../panel-plugin/pulseaudio-menu.c:628
msgid "Input"
msgstr "Entrada"

#: ../panel-plugin/pulseaudio-menu.c:730
msgid "Choose Playlist"
msgstr "Elija la lista de reproducción"

#. Audio mixers
#: ../panel-plugin/pulseaudio-menu.c:765
msgid "_Audio mixer..."
msgstr "Mezclador de _audio..."

#: ../panel-plugin/pulseaudio-notify.c:190
#, c-format
msgid "Volume %d%c (muted)"
msgstr "Volumen: %d%c (silenciado)"

