#, c-format
msgid ""
"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Громкость %d%% (заглушено)</b>\n<small>%s</small>"

#: panel-plugin/pulseaudio-button.c:368
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Громкость %d%%</b>\n<small>%s</small>"

#: panel-plugin/pulseaudio-menu.c:479
msgid "Port"
msgstr "Порт"

#: panel-plugin/pulseaudio-menu.c:580
msgid "Output"
msgstr "Вывод"

#: panel-plugin/pulseaudio-menu.c:624
msgid "Input"
msgstr "Вход"

#: panel-plugin/pulseaudio-menu.c:726
msgid "Choose Playlist"
msgstr "Выберите список воспроизведения"

