msgid "Port"
msgstr ""

#: panel-plugin/pulseaudio-menu.c:580
msgid "Output"
msgstr "เสียงออก"

#: panel-plugin/pulseaudio-menu.c:624
msgid "Input"
msgstr "เสียงเข้า"

#: panel-plugin/pulseaudio-menu.c:726
msgid "Choose Playlist"
msgstr "เลือกผังเล่น"

