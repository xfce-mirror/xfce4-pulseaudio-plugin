"<b>Volume %d%% (muted)</b>\n"
"<small>%s</small>"
msgstr "<b>Lydstyrke %d%% (muted)</b>\n<small>%s</small>"

#: ../panel-plugin/pulseaudio-button.c:372
#, c-format
msgid ""
"<b>Volume %d%%</b>\n"
"<small>%s</small>"
msgstr "<b>Lydstyrke %d%%</b>\n<small>%s</small>"

